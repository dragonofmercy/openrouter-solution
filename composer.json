    "keywords" : ["error", "page", "laravel", "openrouter", "openai"],
    "authors" : [{
        "name" : "DragonOfMercy",
        "email" : "dragonofmercy@gmail.com",
        "role" : "Developer"
    }],
    "homepage" : "https://github.com/dragonofmercy/openrouter-solution",
    "license" : "MIT",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0",
        "openai-php/client": "^0.10.3",
